<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fujias.business.dao.s09.S0900101PasswordMapper">
    
    <select id="checkPassword" parameterType="com.fujias.business.forms.s09.S0900101PasswordForm"
        resultType="Integer">
        SELECT count(*)
        from musers
        where username = #{username}
        and password = #{oldpassword}
    </select>
    
</mapper>
